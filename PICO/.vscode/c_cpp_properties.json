{
    "configurations": [
        {
            "name": "Pico-RP2040",
            "compileCommands": "${workspaceFolder}/spotmicro-rp2040/build/compile_commands.json",
            "includePath": [
                "${workspaceFolder}/spotmicro-rp2040/**",
                "${workspaceFolder}/spotmicro-rp2040/pico-sdk/**"
            ],
            "defines": [],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-arm"
        },
        {
            "name": "Arduino-Nano-RP2040",
            "includePath": [
                "${workspaceFolder}/spotmicro-rp2040/nano_imu/**",
                "${env:LOCALAPPDATA}/Arduino15/packages/arduino/hardware/mbed_rp2040/**",
                "${env:LOCALAPPDATA}/Arduino15/packages/arduino/tools/**"
            ],
            "defines": [
                "ARDUINO_ARCH_MBED",
                "ARDUINO=10819"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-arm"
        }
    ],
    "version": 4
}
